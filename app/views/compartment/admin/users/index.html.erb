<h1>Listing users</h1>

<%= link_to 'New User', new_admin_user_path, class: 'btn pull-right' %>

<table>
  <tr>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= link_to 'Show', admin_user_path(user) %></td>
    <td><%#= link_to 'Edit', edit_admin_user_path(user) %></td>
    <td><%#= link_to 'Destroy', admin_user(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

